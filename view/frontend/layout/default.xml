<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <container name="vaimo.mytest.popup" htmlTag="div" htmlId="eventPopup" before="-">
            <uiComponent name="funnyorderfront_form"/>
        </container>
        <referenceContainer name="header.panel">
            <referenceBlock name="header" remove="true"/>
        </referenceContainer>
        <referenceBlock name="header.links">
            <block class="Magento\Framework\View\Element\Template" name="popup.event.button" template="Vaimo_Mytest::uiform/button.phtml"  before="-"/>
        </referenceBlock>
        <referenceContainer name="vaimo.mytest.popup" display="false"/>
    </body>
</page>